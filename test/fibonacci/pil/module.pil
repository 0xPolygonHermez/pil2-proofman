// require "std_range_check.pil";

const int MODULE_ID = 1;

public mod;

air Module(const int N = 2**8) {

    col witness x, q, x_mod;

    x === q * mod + x_mod;

    // Ensure that x_mod < mod <--> 0 < mod - x_mod
    // range_check(0, colu: mod - x_mod, min: 1, max: N);

    permutation_proves(MODULE_ID, cols: [x, x_mod]);
}