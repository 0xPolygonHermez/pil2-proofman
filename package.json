{
  "name": "pil2-components",
  "version": "0.0.1",
  "description": "pil2 components library",
  "main": "index.js",
  "scripts": {
    "build:basic:zkasm": "CDIR=components/basic/compiler && npx jison $CDIR/zkasm_parser.jison -o $CDIR/zkasm_parser.js && npx jison $CDIR/command_parser.jison -o $CDIR/command_parser.js ",
    "build:basic:rom": "npm run build:basic:zkasm && BDIR=components/basic && node $BDIR/compiler/zkasm.js $BDIR/rom/rom.zkasm -o $BDIR/rom/rom.json",
    "build:basic:rom_compiled": "npm run build:basic:rom && BDIR=components/basic && node $BDIR/tools/rom_json_to_pil2.js > $BDIR/pil/rom_compiled.pil",
    "build:basic": "npm run build:basic:rom_compiled && BDIR=components/basic && npx pil2-compiler components/basic/pil/basic.pil -I lib/tools/pil/,components/mem/pil,lib/std/pil -o components/basic/pil/basic.pilout",
    "build:main:zkasm": "CDIR=components/main/compiler && npx jison $CDIR/zkasm_parser.jison -o $CDIR/zkasm_parser.js && npx jison $CDIR/command_parser.jison -o $CDIR/command_parser.js ",
    "build:main:rom": "npm run build:main:zkasm && BDIR=components/main && node $BDIR/compiler/zkasm.js $BDIR/rom/rom.zkasm -o $BDIR/rom/rom.json",
    "build:main:rom_compiled": "npm run build:main:rom && BDIR=components/main && node $BDIR/tools/rom_json_to_pil2.js > $BDIR/pil/rom_compiled.pil",
    "build:main": "npm run build:main:rom_compiled && BDIR=components/main && npx pil2-compiler components/main/pil/main.pil -I lib/tools/pil/,components/mem/pil,lib/std/pil -o components/main/pil/main.pilout",
    "test:basic": "mocha test/basic/basic.test.js"
  },
  "author": "zkronos73@gmail.com",
  "license": "AGPL-3.0",
  "dependencies": {
    "ffjavascript": "^0.2.63",
    "jison": "^0.4.18",
    "lodash": "^4.17.21",
    "pil2-compiler": "github:0xPolygonHermez/pil2-compiler#develop",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "circom_tester": "^0.0.20",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^9.1.3",
    "pil2-proofman": "github:0xPolygonHermez/pil2-proofman-js#basic_vadcop"
  }
}
