{
    "name": "fibv vadcop",
    "pilout": "examples/fibv/data/fibv.pilout",
    "debug": false,
    "only_check": false,
    "provers": {
        "type": "stark_c",
        "settings": {
            "Fibonacci_2": {
                "const_pols_filename": "examples/fibv/config/Fibonacci_2.const",
                "map_const_pols_file": false,
                "const_tree_filename": "examples/fibv/config/Fibonacci_2.consttree",
                "stark_info_filename": "examples/fibv/config/Fibonacci_2.starkinfo.json",
                "verkey_filename": "examples/fibv/config/Fibonacci_2.verkey",
                "chelpers_filename": "examples/fibv/config/Fibonacci_2.chelpers.bin"
            },
            "Fibonacci_4": {
                "const_pols_filename": "examples/fibv/config/Fibonacci_4.const",
                "map_const_pols_file": false,
                "const_tree_filename": "examples/fibv/config/Fibonacci_4.consttree",
                "stark_info_filename": "examples/fibv/config/Fibonacci_4.starkinfo.json",
                "verkey_filename": "examples/fibv/config/Fibonacci_4.verkey",
                "chelpers_filename": "examples/fibv/config/Fibonacci_4.chelpers.bin"
            },
            "Module": {
                "const_pols_filename": "examples/fibv/config/Module.const",
                "map_const_pols_file": false,
                "const_tree_filename": "examples/fibv/config/Module.consttree",
                "stark_info_filename": "examples/fibv/config/Module.starkinfo.json",
                "verkey_filename": "examples/fibv/config/Module.verkey",
                "chelpers_filename": "examples/fibv/config/Module.chelpers.bin"
            }
        }
    }
}
