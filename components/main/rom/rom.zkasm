VAR GLOBAL myvar

start:

        STEP => A
        0 :ASSERT

        2 => B
        3 => A
        A + B => A
        5           :ASSERT
        
        A + 600      :MSTORE(myvar)
        $ => A      :MLOAD(myvar)

        15          :ASSERT

end:
        0 => A,B,C,D,E,CTX, SP, PC

finalWait:
        ${beforeLast()}  : JMPN(finalWait)

                         : JMP(start)
failAssert:
        A + 1 :ASSERT
